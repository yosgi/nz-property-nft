    "deploy:sepolia": "truffle exec scripts/deploy.js --network sepolia",
    "migrate:reset": "truffle migrate --reset",
    "test": "truffle test",
    "console": "truffle console",
    "ganache": "ganache-cli -p 8545 -m 'your twelve word mnemonic here for development only'",
    "deploy:development": "truffle migrate --network development",
    "deploy:ganache": "truffle migrate --network ganache"
